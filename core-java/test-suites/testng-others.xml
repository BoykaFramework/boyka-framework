<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Boyka Framework Other Tests Suite" verbose="2">
    <listeners>
        <listener class-name="io.github.boykaframework.testng.listeners.TestResultListener"/>
    </listeners>
    <test name="Test other features" parallel="classes" group-by-instances="true">
        <packages>
            <package name="io.github.boykaframework.testng.others"/>
        </packages>
    </test>
</suite>
