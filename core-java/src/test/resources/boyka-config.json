      "test_local_wdio_ios": {
        "server": {
          "target": "LOCAL",
          "port": 4726,
          "callback_port": 4726,
          "session_override": true,
          "driver": "XCUI",
          "logging": {
            "debug_spacing": true,
            "timestamp": true,
            "local_timezone": true
          },
          "other_args": {
            "--long-stacktrace": true,
            "--callback-port": 4726,
            "--callback-address": "127.0.0.1"
          }
        },
        "device": {
          "os": "IOS",
          "version": "17.5",
          "name": "iPhone 15",
          "type": "VIRTUAL",
          "server_install_timeout": 60,
          "server_launch_timeout": 60,
          "permissions": {
            "NOTIFICATIONS": "YES",
            "MICROPHONE": "YES"
          },
          "web_view_connect_timeout": 120,
          "web_view_connect_retries": 3,
          "connect_keyboard": false,
          "typing_speed": 35,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "virtual_device": {
            "headless": true,
            "launch_timeout": 240
          },
          "wda": {
            "launch_timeout": 240,
