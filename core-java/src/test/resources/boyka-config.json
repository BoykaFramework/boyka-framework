{
  "$schema": "https://json.schemastore.org/boyka-config.json",
  "listeners_package": "io.github.boykaframework.testng.listeners",
  "data": {
    "path": "data/excel"
  },
  "logging": {
    "logs_folder": "./logs",
    "console_logs": true,
    "archive": {
      "max_days": 1,
      "max_size": 5,
      "size_unit": "MB",
      "on_every_run": true
    }
  },
  "common_setting": {
    "api": {
      "logging": {
        "request": true,
        "response": true
      },
      "timeout": {
        "read_timeout": 10,
        "write_timeout": 10,
        "connection_timeout": 10
      },
      "schema_path": "schema/"
    }
  },
  "ui": {
    "timeout": {
      "implicit_wait": 10,
      "explicit_wait": 30,
      "page_load_timeout": 30,
      "script_timeout": 10,
      "highlight_delay": 100
    },
    "logging": {
      "exclude_logs": [
        "bugreport"
      ]
    },
    "screenshot": {
      "enabled": true,
      "path": "./screenshots",
      "extension": "jpeg",
      "prefix": "SCR"
    },
    "web": {
      "test_local_chrome": {
        "base_url": "https://the-internet.herokuapp.com/",
        "browser": "CHROME",
        "page_load_strategy": "NORMAL",
        "highlight": false,
        "headless": true,
        "resize": "CUSTOM",
        "custom_size": {
          "width": 1580,
          "height": 1080
        },
        "experimental_options": {
          "prefs": {
            "download.default_directory": "${sys:user.home}/Downloads",
            "download.prompt_for_download": false,
            "safebrowsing.enabled": true
          }
        },
        "browser_options": [
          "use-fake-device-for-media-stream",
          "use-fake-ui-for-media-stream"
        ]
      },
      "test_local_en": {
        "base_url": "https://webdriver.io/",
        "browser": "CHROME",
        "resize": "MAXIMIZED",
        "language": {
          "language": "EN"
        },
        "highlight": false,
        "headless": true
      },
      "test_local_de": {
        "base_url": "https://webdriver.io/",
        "browser": "CHROME",
        "resize": "MAXIMIZED",
        "language": {
          "language": "GR"
        },
        "highlight": false,
        "headless": true
      },
      "test_draw": {
        "base_url": "https://excalidraw.com/",
        "browser": "CHROME"
      },
      "test_local_firefox": {
        "browser": "FIREFOX"
      },
      "test_local_edge": {
        "base_url": "https://the-internet.herokuapp.com/",
        "browser": "EDGE",
        "resize": "FULL_SCREEN"
      },
      "test_local_safari": {
        "browser": "SAFARI"
      },
      "test_browserstack_chrome": {
        "browser": "REMOTE",
        "base_url": "https://the-internet.herokuapp.com/",
        "target": "BROWSER_STACK",
        "user_name": "${env:BS_USER}",
        "password": "${env:BS_KEY}",
        "resize": "MAXIMIZED",
        "capabilities": {
          "browser": "Chrome",
          "browserVersion": "latest",
          "os": "Windows",
          "osVersion": "11",
          "resolution": "1920x1080",
          "projectName": "Test Boyka Project",
          "buildName": "Test BrowserStack Build",
          "sessionName": "Test BrowserStack Session",
          "seleniumVersion": "4.8.0",
          "telemetryLogs": "true",
          "seleniumLogs": "true",
          "video": "true",
          "networkLogs": "true",
          "debug": "true"
        }
      },
      "test_selenium_grid": {
        "browser": "REMOTE",
        "base_url": "https://the-internet.herokuapp.com/",
        "target": "LOCAL",
        "port": 4444,
        "capabilities": {
          "browserName": "chrome"
        }
      },
      "test_lambda_test_chrome": {
        "browser": "REMOTE",
        "base_url": "https://the-internet.herokuapp.com/",
        "target": "LAMBDA_TEST_WEB",
        "user_name": "${env:LT_USER}",
        "password": "${env:LT_KEY}",
        "resize": "NORMAL",
        "capabilities": {
          "browserName": "Chrome",
          "version": "latest",
          "platform": "Windows 11",
          "resolution": "1920x1080",
          "build": "Test LambdaTest Build",
          "name": "Test LambdaTest Session",
          "network": true,
          "visual": true,
          "video": true,
          "console": true
        }
      },
      "test_lambda_test_no_username": {
        "browser": "REMOTE",
        "target": "LAMBDA_TEST_WEB",
        "protocol": "HTTPS",
        "host": "hub.lambdatest.com",
        "password": "${env:LT_KEY}",
        "capabilities": {
          "browserName": "Chrome",
          "version": "99.0",
          "platform": "Windows 10",
          "resolution": "1920x1080",
          "build": "Test LambdaTest Build",
          "name": "Test LambdaTest Session",
          "network": true,
          "visual": true,
          "video": true,
          "console": true
        }
      },
      "test_lambda_test_no_password": {
        "browser": "REMOTE",
        "target": "LAMBDA_TEST_WEB",
        "protocol": "HTTPS",
        "host": "hub.lambdatest.com",
        "user_name": "${env:LT_USER}",
        "capabilities": {
          "browserName": "Chrome",
          "version": "latest",
          "platform": "Windows 11",
          "resolution": "1920x1080",
          "build": "Test LambdaTest Build",
          "name": "Test LambdaTest Session",
          "network": true,
          "visual": true,
          "video": true,
          "console": true
        }
      },
      "test_lambda_test_no_host": {
        "browser": "REMOTE",
        "protocol": "HTTPS",
        "target": null,
        "user_name": "${env:LT_USER}",
        "password": "${env:LT_KEY}",
        "capabilities": {
          "browserName": "Chrome",
          "version": "latest",
          "platform": "Windows 11",
          "resolution": "1920x1080",
          "build": "Test LambdaTest Build",
          "name": "Test LambdaTest Session",
          "network": true,
          "visual": true,
          "video": true,
          "console": true
        }
      },
      "test_local_empty_browser": {
        "browser": "ABC"
      },
      "test_local_null_browser": {},
      "test_local_none_browser": {
        "browser": "NONE"
      },
      "test_chrome_no_base_url": {
        "browser": "CHROME"
      }
    },
    "desktop": {
      "test_mac": {
        "server": {
          "target": "LOCAL",
          "port": 4729,
          "session_override": true,
          "driver": "MAC"
        },
        "machine": {
          "os": "MAC",
          "version": "15.4",
          "application": {
            "bundle_id": "com.apple.calculator"
          },
          "video": {
            "enabled": true,
            "time_limit": 300,
            "preset": "VERY_FAST",
            "device_id": 2,
            "mac": {
              "capture_clicks": true,
              "fps": 10
            }
          }
        }
      }
    },
    "mobile": {
      "test_local_sauce_android": {
        "server": {
          "target": "LOCAL",
          "port": 4723,
          "session_override": true,
          "driver": "UI_AUTOMATOR",
          "allow_insecure": [
            "get_server_logs",
            "chromedriver_autodownload"
          ]
        },
        "device": {
          "os": "ANDROID",
          "version": "14",
          "name": "Pixel_8_Pro",
          "type": "VIRTUAL",
          "server_install_timeout": 60,
          "server_launch_timeout": 60,
          "ignore_unimportant_views": true,
          "full_reset": true,
          "system_port": 8200,
          "language": {
            "language": "DE"
          },
          "swipe": {
            "max_swipe_until_found": 5
          },
          "application": {
            "path": "/apps/android/sauce-demo-i18n.apk",
            "wait_activity": "com.swaglabsmobileapp.MainActivity",
            "install_timeout": 180,
            "wait_timeout": 120
          },
          "virtual_device": {
            "name": "Pixel_8_Pro",
            "headless": true
          }
        }
      },
      "test_local_wdio_android": {
        "server": {
          "target": "LOCAL",
          "port": 4724,
          "external": true,
          "driver": "UI_AUTOMATOR"
        },
        "device": {
          "os": "ANDROID",
          "version": "14",
          "name": "Pixel_8_Pro",
          "type": "VIRTUAL",
          "server_install_timeout": 60,
          "auto_grant_permissions": true,
          "system_port": 8201,
          "server_launch_timeout": 60,
          "ignore_unimportant_views": true,
          "full_reset": true,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "video": {
            "enabled": true,
            "time_limit": 300,
            "size": "800x720",
            "android": {
              "bit_rate": 4
            }
          },
          "application": {
            "path": "/apps/android/wdio-demo.apk",
            "type": "HYBRID",
            "install_timeout": 180,
            "wait_timeout": 120
          },
          "virtual_device": {
            "name": "Pixel_8_Pro",
            "headless": true
          }
        }
      },
      "test_grid_wdio_android": {
        "server": {
          "target": "LOCAL",
          "port": 4444,
          "driver": "UI_AUTOMATOR",
          "external": true
        },
        "device": {
          "os": "ANDROID",
          "version": "14",
          "name": "Pixel_8_Pro",
          "type": "VIRTUAL",
          "server_install_timeout": 60,
          "server_launch_timeout": 60,
          "ignore_unimportant_views": true,
          "full_reset": true,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "application": {
            "path": "/apps/android/wdio-demo.apk",
            "type": "HYBRID",
            "install_timeout": 180,
            "wait_timeout": 120
          },
          "virtual_device": {
            "name": "Pixel_8_Pro",
            "headless": true
          }
        }
      },
      "test_bs_android": {
        "server": {
          "target": "BROWSER_STACK",
          "base_path": "/wd/hub",
          "user_name": "${env:BS_USER}",
          "password": "${env:BS_KEY}",
          "driver": "UI_AUTOMATOR"
        },
        "device": {
          "os": "ANDROID",
          "version": "14.0",
          "name": "Google Pixel 8 Pro",
          "type": "CLOUD",
          "ignore_unimportant_views": true,
          "application": {
            "path": "AndroidApp",
            "wait_activity": "com.swaglabsmobileapp.MainActivity",
            "external": true,
            "install_timeout": 180
          },
          "capabilities": {
            "projectName": "BrowserStack Android Project",
            "buildName": "Test BrowserStack Build",
            "sessionName": "Test BrowserStack Session",
            "appiumVersion": "2.6.0",
            "automationVersion": "latest",
            "deviceLogs": true,
            "networkLogs": true,
            "debug": true,
            "video": true,
            "appiumLogs": true
          },
          "swipe": {
            "max_swipe_until_found": 5
          }
        }
      },
      "test_lt_android": {
        "server": {
          "target": "LAMBDA_TEST_MOBILE",
          "base_path": "/wd/hub",
          "user_name": "${env:LT_USER}",
          "password": "${env:LT_KEY}",
          "driver": "UI_AUTOMATOR"
        },
        "device": {
          "type": "CLOUD",
          "application": {
            "install_timeout": 180,
            "wait_activity": "com.swaglabsmobileapp.MainActivity"
          },
          "ignore_unimportant_views": true,
          "capabilities": {
            "platformName": "Android",
            "deviceName": "Pixel 8 Pro",
            "platformVersion": "14",
            "app": "AndroidApp",
            "project": "LambdaTest Android Project",
            "build": "Test LambdaTest Build",
            "name": "Test LambdaTest Session",
            "devicelog": true,
            "network": true,
            "visual": true,
            "video": true,
            "autoGrantPermissions": true,
            "autoAcceptAlerts": true,
            "isRealMobile": true,
            "w3c": true
          },
          "swipe": {
            "max_swipe_until_found": 5
          }
        }
      },
      "test_local_android_web": {
        "server": {
          "target": "LOCAL",
          "port": 4728,
          "session_override": true,
          "driver": "UI_AUTOMATOR",
          "allow_insecure": [
            "get_server_logs",
            "chromedriver_autodownload"
          ]
        },
        "device": {
          "os": "ANDROID",
          "version": "14",
          "name": "Pixel_8_Pro",
          "type": "VIRTUAL",
          "server_install_timeout": 120,
          "system_port": 8202,
          "server_launch_timeout": 120,
          "adb_timeout": 120,
          "no_reset": true,
          "swipe": {
            "max_swipe_until_found": 5
          },
          "video": {
            "enabled": true,
            "time_limit": 300,
            "size": "800x720",
            "android": {
              "bit_rate": 4
            }
          },
          "application": {
            "browser": "CHROME",
            "type": "WEB",
            "base_url": "https://the-internet.herokuapp.com/"
          },
          "virtual_device": {
            "name": "Pixel_8_Pro",
            "headless": true
          }
        }
      },
      "test_local_sauce_ios": {
        "server": {
          "target": "LOCAL",
          "port": 4725,
          "session_override": true,
          "driver": "XCUI"
        },
        "device": {
          "os": "IOS",
          "version": "18.1",
          "name": "iPhone 15",
          "type": "VIRTUAL",
          "server_install_timeout": 60,
          "server_launch_timeout": 60,
          "web_view_connect_timeout": 120,
          "web_view_connect_retries": 3,
          "connect_keyboard": false,
          "typing_speed": 25,
          "language": {
            "language": "GR"
          },
          "swipe": {
