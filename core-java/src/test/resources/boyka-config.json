          "swipe": {
            "distance": 25,
            "max_swipe_until_found": 5
          },
          "application": {
            "path": "/apps/android/wdio-demo.apk",
            "wait_activity": "com.wdiodemoapp.MainActivity",
            "install_timeout": 180
          },
          "virtual_device": {
            "name": "Pixel_6_Pro",
            "headless": true
          }
        }
      },
      "test_bs_android": {
        "server": {
          "cloud": "BROWSER_STACK",
          "protocol": "HTTPS",
          "host": "hub-cloud.browserstack.com",
          "user_name": "${env:BS_USER}",
          "password": "${env:BS_KEY}",
